{
    (let egg (fun (bar) (print bar)))
    (let data ["Iron Man" "is" "Tony Stark"])
    (mut callbacks [])

    (mut acc 0)
    (while (!= acc (len data)) {
        (mut d (@ data acc))
        (set callbacks (append callbacks (fun (&d) (egg d))))
        (set acc (+ 1 acc))
    })

    (set acc 0)
    (while (!= acc (len callbacks)) {
        (print "stored: " (@ callbacks acc) .d)
        ((@ callbacks acc))
        (set acc (+ 1 acc))
    })
}
