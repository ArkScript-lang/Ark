!{o 12}
(print o)

!{if (= o 12)
    (print "12")
    (print "not 12")
}

!{if (= o 12)
    !{a (...args) (print args)}
    !{a (...args) (print "no")}
}

(a 1 2 3)

!{if (and true true)
    (print "expected true")
    (assert false "!{if (and true true)} failed")
}

!{if (or false true)
    (print "expected true")
    (assert false "!{if (or false true)} failed")
}

!{if (= true (and true true))
    (print "expected true")
    (assert false "!{if (= true (and true true))} failed")
}