(let ackermann (fun (m n) {
    (if (> m 0)
        # then
        (if (= 0 n)
            # then
            (ackermann (- m 1) 1)
            # else
            (ackermann (- m 1) (ackermann m (- n 1)))
        )
        # else
        (+ 1 n)
    )
}))

(mut i 0)
(mut t (time))
(while (< i 100) (begin
    (ackermann 3 6)
    (set i (+ 1 i))))
(print (* (/ (- (time) t) 100) 1000) "ms")