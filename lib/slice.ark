{
  (let slice (fun (string escapeCharacter){
    #made by Natendrtfm <3
        (mut i 0)
        (mut word "")
        (mut letter "")
        (mut sliccedList [])
        (while (< i (len string) ){
            (set letter (@ string i))
            (if (= letter escapeCharacter){(set sliccedList (append sliccedList word))(set word "")} ())
            (set word (+ word letter))
            (set i (+ i 1))
        })
        (append sliccedList word)
  }))
}    
