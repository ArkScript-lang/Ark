{
    (let abs (fun (x)
        (if (< x 0)
            (* -1 x)
            x
        )))
    
    (let pow (fun (x a) (exp (* a (ln x)))))

    (let sqrt (fun (x) (exp (* 0.5 (ln x)))))

    (let min (fun (a b)
        (if (< a b)
            a
            b
        )))

    (let max (fun (a b)
        (if (> a b)
            a
            b
        )))

    (let even (fun (n)
        (= 0 (mod n 2))))

    (let odd (fun (n)
        (= 1 (abs (mod n 2)))))
}
