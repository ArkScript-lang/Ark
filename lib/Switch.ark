(let switch (fun (value tests) {
    (let acc 0)
    (let end (len tests))

    (while (!= acc end) {
        (let r (@ tests acc))
        (let bis (@ r 0))
        (if (= bis value)
            {
                (let df (@ r 1))
                (df)
                (set acc (- end 1))
            }
            ()
        )

        (set acc (+ 1 acc))
    })
}))