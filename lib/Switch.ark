(let switch (fun (value tests) {
    (mut acc 0)
    (let end (len tests))

    (while (!= acc end) {
        (mut r (@ tests acc))
        (mut bis (@ r 0))
        (if (= bis value)
            {
                (mut df (@ r 1))
                (df)
                (set acc (- end 1))
            }
            ()
        )

        (set acc (+ 1 acc))
    })
}))