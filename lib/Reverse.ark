    (let reverse (fun (inputed){
        (assert (!= inputed "") "Argument length must be superior to 0") # Sounds weird to me , but it work ! 
        (mut i (- (len inputed) 1))
        (mut returnedString "")
        (while (> i -1){
            (set returnedString (+ returnedString (@ inputed i)))
            (set i (- i 1))
        })
        returnedString
    }))
}