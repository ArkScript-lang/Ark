(begin
    (def fibo
        (fun (n) (if (= n 0)
                    0 ' then
                    (if (= n 1) ' else
                        1 ' then
                        (+ ' else
                            (fibo (- n 1))
                            (fibo (- n 2))
                        )
                    ) ' endif
                ) ' endif
        ) ' endfun
    ) ' enddef

    (print (fibo 2))
)
