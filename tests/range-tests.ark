{
    (import "Range.ark")

    (let range-tests (fun () {
        (mut tests 0)

        (let r (range 5 10))
    
        (assert (= 5 (r)) "Range test 1 failed")        (set tests (+ 1 tests))
        (assert (= 6 (r)) "Range test 1°2 failed")      (set tests (+ 1 tests))
        (assert (= 7 (r)) "Range test 1°3 failed")      (set tests (+ 1 tests))
        (assert (= 8 (r)) "Range test 1°4 failed")      (set tests (+ 1 tests))
        (assert (= 9 (r)) "Range test 1°5 failed")      (set tests (+ 1 tests))
        (assert (= nil (r)) "Range test 1°6 failed")        (set tests (+ 1 tests))

        (print "  Range tests passed \t\t(" tests ")")
        tests
    }))

    (let passed-range (range-tests))
}