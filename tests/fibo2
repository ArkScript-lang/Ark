(begin
    {def fibo
        (fun (n)
            [begin
                (def a 0)
                (def b 1)
                {while (< a n)
                    [begin
                        (def c (+ a b))
                        (set a b)
                        (set b c)
                    ]
                }
                ' value to return
                a
            ]
        )
    }
    (print (fibo 10000000))
)
