(begin
    {let fibo
        (fun (n)
            [begin
                (let a 0)
                (let b 1)
                {while (< a n)
                    [begin
                        (let c (+ a b))
                        (set a b)
                        (set b c)
                    ]
                }
                ' value to return
                a
            ]
        )
    }
    (print (fibo 10000000))
)
