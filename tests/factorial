(begin
    (hastype fact (Number) Number)
    (let fact (fun (n) (begin
        (let a 1)
        (let acc 2)
        (while (!= acc n)
            (begin
                (set a (* a acc))
                (set acc (+ 1 acc))
            )
        )
        ' return value:
        a
    )))
    (fact 10000)
)