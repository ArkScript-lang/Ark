(begin
    (hastype fact (Number) Number)
    (def fact [fun (n) {begin
        (def a 1)
        (def acc 2)
        (while [!= acc n]
            (begin
                [set a [* a acc]]
                [set acc [+ 1 acc]]
            )
        )
        ' return value:
        a
    }])
    (print (fact 1000))
)