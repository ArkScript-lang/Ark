{
    (import "Math/Arithmetic.ark")

    (let math-tests (fun () {
        (mut tests 0)

        (assert (= 3 3.0) "Math test basic failed")             (set tests (+ 1 tests))

        (assert (= 12 (+ 5.5 6.5)) "Math test 1 failed")        (set tests (+ 1 tests))
        (assert (= 12 (+ 6 6)) "Math test 1°2 failed")          (set tests (+ 1 tests))

        (assert (= 12 (- 14.5 2.5)) "Math test 2 failed")       (set tests (+ 1 tests))
        (assert (= 12 (- 111 99)) "Math test 2°2 failed")       (set tests (+ 1 tests))

        (assert (= 12 (* 3 4)) "Math test 3 failed")            (set tests (+ 1 tests))
        (assert (= -12 (* -3 4)) "Math test 3°2 failed")        (set tests (+ 1 tests))

        (assert (= 12 (/ 144 12)) "Math test 4 failed")         (set tests (+ 1 tests))
        (assert (= 3.0 (/ 7.5 2.5)) "Math test 4°2 failed")     (set tests (+ 1 tests))

        (assert (< 12 14) "Math test 5 failed")                 (set tests (+ 1 tests))
        (assert (< -12 14) "Math test 5°2 failed")              (set tests (+ 1 tests))

        (assert (> 112 14) "Math test 6 failed")                (set tests (+ 1 tests))
        (assert (> -12 -14) "Math test 6°2 failed")             (set tests (+ 1 tests))

        (assert (!= 12 14) "Math test 7 failed")                (set tests (+ 1 tests))
        (assert (!= -12 14) "Math test 7°2 failed")             (set tests (+ 1 tests))

        (assert (<= 12 12) "Math test 8 failed")                (set tests (+ 1 tests))
        (assert (<= -12 14) "Math test 8°2 failed")             (set tests (+ 1 tests))

        (assert (= 0 (mod 10 5)) "Math test 9 failed")          (set tests (+ 1 tests))
        (assert (= 0.5 (mod 7.5 7)) "Math test 9°2 failed")     (set tests (+ 1 tests))

        (assert (= 6 (+ 1 2 3)) "Math test 10 failed")          (set tests (+ 1 tests))
        (assert (= -4 (- 1 2 3)) "Math test 10°2 failed")       (set tests (+ 1 tests))
        (assert (= 24 (* 2 3 4)) "Math test 10°3 failed")       (set tests (+ 1 tests))
        (assert (= 2 (/ 32 8 2)) "Math test 10°4 failed")       (set tests (+ 1 tests))
        (assert (= 2 (mod 12 7 3)) "Math test 10°5 failed")     (set tests (+ 1 tests))
        (assert (and true true true) "Math test 10°6 failed")   (set tests (+ 1 tests))
        (assert (or false false true) "Math test 10°7 failed")  (set tests (+ 1 tests))

        (assert (= (exp 0) 1) "Math test 11 failed")            (set tests (+ 1 tests))
        (assert (= (ln 1) 0) "Math 11°2 failed")                (set tests (+ 1 tests))
        (assert (= E (exp 1)) "Math test 11°3 failed")          (set tests (+ 1 tests))
        (assert (= Tau (* 2 Pi)) "Math test 11°4 failed")       (set tests (+ 1 tests))
        (assert (isNaN NaN) "Math test 11°5 failed")            (set tests (+ 1 tests))
        (assert (isInf Inf) "Math test 11°6 failed")            (set tests (+ 1 tests))
        (assert (= false (isNaN 0)) "Math test 11°7 failed")    (set tests (+ 1 tests))
        (assert (= false (isInf 0)) "Math test 11°8 failed")    (set tests (+ 1 tests))

        (assert (= (pow 2 2) 4) "Math test 12 failed")          (set tests (+ 1 tests))
        (assert (= (sqrt 4) 2) "Math test 12°2 failed")         (set tests (+ 1 tests))
        (assert (= (abs -1) 1) "Math test 12°3 failed")         (set tests (+ 1 tests))
        (assert (= (abs 1) 1) "Math test 12°4 failed")          (set tests (+ 1 tests))
        (assert (= (min 2 3) 2) "Math test 12°5 failed")        (set tests (+ 1 tests))
        (assert (= (max 2 3) 3) "Math test 12°6 failed")        (set tests (+ 1 tests))

        (print "  Math tests passed \t\t(" tests ")")
        tests
    }))

    (let passed-arithmetic (math-tests))
}
